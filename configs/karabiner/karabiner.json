{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Remap enter to shift+enter, and remap command+enter to enter in Microsoft Teams",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.teams$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "return_or_enter",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "return_or_enter",
                                        "modifiers": ["left_shift"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.teams$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "return_or_enter",
                                    "modifiers": { "mandatory": ["left_command"] }
                                },
                                "to": [{ "key_code": "return_or_enter" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな）",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_command" }],
                                "to_if_alone": [{ "key_code": "japanese_eisuu" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "right_command" }],
                                "to_if_alone": [{ "key_code": "japanese_kana" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change left_option+hjkl to arrow keys",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "left_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "j",
                                    "modifiers": {
                                        "mandatory": ["left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "down_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "k",
                                    "modifiers": {
                                        "mandatory": ["left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "up_arrow" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "l",
                                    "modifiers": {
                                        "mandatory": ["left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "right_arrow" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Ctrl+[を押したときに、escキーと英数キーを送信する",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "keyboard_types": ["ansi", "iso"],
                                        "type": "keyboard_type_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "open_bracket",
                                    "modifiers": { "mandatory": ["control"] }
                                },
                                "to": [
                                    { "key_code": "escape" },
                                    { "key_code": "japanese_eisuu" }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "keyboard_types": ["jis"],
                                        "type": "keyboard_type_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "close_bracket",
                                    "modifiers": { "mandatory": ["control"] }
                                },
                                "to": [
                                    { "key_code": "escape" },
                                    { "key_code": "japanese_eisuu" }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "escキーを押したときに、英数キーも送信する（vim用）",
                        "manipulators": [
                            {
                                "from": { "key_code": "escape" },
                                "to": [
                                    { "key_code": "escape" },
                                    { "key_code": "japanese_eisuu" }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "ctrl+tキーを押したときに、英数キーも送信する（tmux用）",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "t",
                                    "modifiers": { "mandatory": ["control"] }
                                },
                                "to": [
                                    {
                                        "key_code": "t",
                                        "modifiers": ["control"]
                                    },
                                    { "key_code": "japanese_eisuu" }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Double tap 'right_shift' to open Terminal",
                        "enabled": false,
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.github\\.wez\\.wezterm$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    },
                                    {
                                        "name": "right_shift_key",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "right_shift" },
                                "to": [{ "shell_command": "osascript -e 'tell application \"WezTerm\" to activate' &" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.github\\.wez\\.wezterm$"
                                        ],
                                        "type": "frontmost_application_if"
                                    },
                                    {
                                        "name": "right_shift_key",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": { "key_code": "right_shift" },
                                "to": [{ "shell_command": "osascript -e 'tell application \"Safari\" to activate' &" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "right_shift_key",
                                        "type": "variable_if",
                                        "value": 0
                                    }
                                ],
                                "from": {
                                    "key_code": "right_shift",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "right_shift_key",
                                            "value": 1
                                        }
                                    },
                                    { "key_code": "right_shift" }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "right_shift_key",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
                                        {
                                            "set_variable": {
                                                "name": "right_shift_key",
                                                "value": 0
                                            }
                                        }
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "disable_built_in_keyboard_if_exists": true,
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 514,
                        "vendor_id": 1278
                    },
                    "manipulate_caps_lock_led": false
                },
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 33,
                        "vendor_id": 1278
                    },
                    "manipulate_caps_lock_led": false
                },
                {
                    "disable_built_in_keyboard_if_exists": true,
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 256,
                        "vendor_id": 2131
                    },
                    "manipulate_caps_lock_led": false
                },
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 591,
                        "vendor_id": 1452
                    },
                    "simple_modifications": [
                        {
                            "from": { "key_code": "backslash" },
                            "to": [{ "key_code": "delete_or_backspace" }]
                        },
                        {
                            "from": { "key_code": "delete_forward" },
                            "to": [{ "key_code": "grave_accent_and_tilde" }]
                        },
                        {
                            "from": { "key_code": "delete_or_backspace" },
                            "to": [{ "key_code": "backslash" }]
                        }
                    ]
                }
            ],
            "fn_function_keys": [
                {
                    "from": { "key_code": "f3" },
                    "to": [{ "key_code": "mission_control" }]
                },
                {
                    "from": { "key_code": "f4" },
                    "to": [{ "key_code": "launchpad" }]
                },
                {
                    "from": { "key_code": "f5" },
                    "to": [{ "key_code": "illumination_decrement" }]
                },
                {
                    "from": { "key_code": "f6" },
                    "to": [{ "key_code": "illumination_increment" }]
                },
                {
                    "from": { "key_code": "f9" },
                    "to": [{ "consumer_key_code": "fastforward" }]
                }
            ],
            "name": "Default profile",
            "selected": true,
            "simple_modifications": [
                {
                    "from": { "key_code": "caps_lock" },
                    "to": [{ "key_code": "left_control" }]
                }
            ],
            "virtual_hid_keyboard": {
                "caps_lock_delay_milliseconds": 0,
                "country_code": 0,
                "keyboard_type": "ansi",
                "keyboard_type_v2": "ansi"
            }
        }
    ]
}